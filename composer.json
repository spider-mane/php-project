{
  "name": "webtheory/php-project",
  "type": "project",
  "description": "Simple starter package for any php project",
  "homepage": "https://github.com/spider-mane/php-project",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Williams",
      "email": "spider.mane.web@gmail.com",
      "homepage": "https://github.com/spider-mane",
      "role": "Developer"
    }
  ],
  "keywords": [
    "webtheory",
    "php-project",
    "php-skeleton",
    "php",
    "skeleton",
    "project",
    "starter"
  ],
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "fakerphp/faker": "^1.13",
    "filp/whoops": "^2.5",
    "friendsofphp/php-cs-fixer": "^2.15",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.3",
    "phpbench/phpbench": "@dev",
    "phpstan/phpstan": "^0.12.83",
    "phpunit/phpunit": "^9.5",
    "psy/psysh": "^0.10.7",
    "spatie/phpunit-watcher": "^1.12",
    "squizlabs/php_codesniffer": "^3.0",
    "symfony/var-dumper": "^4.3",
    "vimeo/psalm": "^4.7"
  },
  "autoload": {
    "psr-4": {
      "PseudoVendor\\PseudoPackage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\": "tests/unit/",
      "Tests\\Acceptance\\": "tests/acceptance/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "watch": "vendor/bin/phpunit-watcher",
    "check": "vendor/bin/phpcs",
    "fix": "vendor/bin/php-cs-fixer",
    "analyze": "vendor/bin/psalm",
    "bench": "vendor/bin/phpbench",
    "play": "vendor/bin/psysh",
    "collect": "vendor/bin/var-dump-server",
    "build": [
      "@fix",
      "@check",
      "@analyze",
      "@test"
    ],
    "post-create-project-cmd": [
      "bin/composer-prefill"
    ]
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
